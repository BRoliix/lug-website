{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "outDir": "lib",
    "noImplicitReturns": true,
    "noUnusedLocals": true
  },
  "compileOnSave": true,
  "include": ["src"],
  "exclude": [
    "../../.next",
    "../../node_modules",
    "node_modules"
  ]
}
